      "outDir": "./build/renderer",
      "rootDir": "./src/renderer",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src/renderer/**/*"],
    "exclude": ["node_modules", "build", "static"]
}  